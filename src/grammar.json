{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "fraz",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "definition"
      }
    },
    "definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "object"
            },
            {
              "type": "STRING",
              "value": "has"
            },
            {
              "type": "SYMBOL",
              "name": "attribute"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "attribute"
            },
            {
              "type": "STRING",
              "value": "is"
            },
            {
              "type": "SYMBOL",
              "name": "type"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "attribute"
            },
            {
              "type": "STRING",
              "value": "either"
            },
            {
              "type": "SYMBOL",
              "name": "attribute"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "attribute"
            },
            {
              "type": "STRING",
              "value": "test"
            },
            {
              "type": "SYMBOL",
              "name": "attribute"
            }
          ]
        }
      ]
    },
    "object": {
      "type": "PATTERN",
      "value": "[A-Z][a-z]*"
    },
    "attribute": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "# "
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "type": {
      "type": "PATTERN",
      "value": "datetime|integer"
    },
    "integer_literal": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "string_literal": {
      "type": "PATTERN",
      "value": "[a-z]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
